@model IEnumerable<Ticket_Sales.Models.DB.ApplicationUser>
@{
    ViewData["Title"] = "Index";
     Layout = "~/Areas/Admin/Views/Shared/_LayoutAdmin.cshtml";
}

<h1>Danh sách tài khoản</h1>
<table class="table">
    <thead>
        <tr>
            <th>User ID</th>
            <th>FullName</th>
            <th>Email</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>@item.Id</td>
                <td>@item.FullName</td>
                <td>@item.Email</td>
                <td>
                    <a asp-action="Delete" asp-route-userId="@item.Id" class="btn btn-danger">Delete</a>
                </td>
            </tr>
        }
    </tbody>
</table>


